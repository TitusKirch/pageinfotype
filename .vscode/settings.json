{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "javascript.format.semicolons": "remove",
    "typescript.format.semicolons": "remove",
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
}